<!DOCTYPE html>
<html>
<head>
</head>
<body>
  <h1>Products Display!</h1>
  <ul>
    <% products.forEach(product => { %>
      <li>
        <h2><%= product.name %></h2>
        <p><%= product.image %></p>
        <p><%= product.description %></p>
        <p>Price: $<%= product.price %></p>
        <!-- Add more product details as needed -->
      </li>
    <% }); %>
  </ul>
</body>
</html>
